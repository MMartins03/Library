extends layout

block content
  h1= title
  
  ul
    - dish_list.sort(function(a, b) {let textA = a.title.toUpperCase(); let textB = b.title.toUpperCase(); return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;});
    each dish in dish_list
      li 
        a(href=dish.url) #{dish.title}
        |  (#{dish.chef.name})

    else
      li There are no dishs.
